{
	"version": "0.0.1",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/src/index.js",
            "stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {},
			"console":"internalConsole",
			"sourceMaps": false,
			"outFiles": []
		},
        {
            "name": "Launch Tests (Jasmine)",
			"type": "node",
			"request": "launch",
            "program": "${workspaceRoot}/spec/executeTests.js",
			"sourceMaps": false,
			"cwd": "${workspaceRoot}",
			"outputCapture": "std",
			"skipFiles": [
				"<node_internals>/**",
				"node_modules/cls-hooked/**"
			],
			"outFiles": [],
			"stopOnEntry": false,
			"env": {
			}
		}
	]
}